{
    "name": "manojkiran/actionbuttons",
    "description": "Most commanly used buttons in Laravel for CRUD operations",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Manojkiran Appathurai",
            "email": "manojkiran10031998@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "illuminate/database": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "laravelcollective/html": "^5.8|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.15|^8.4|^9.0",
        "laravel/framework": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Manojkiran\\ActionButtons\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Manojkiran\\ActionButtons\\Tests\\":"tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Manojkiran\\ActionButtons\\ActionServiceProvider"
            ],
            "aliases": {
                "ActionButton": "Manojkiran\\ActionButtons\\Facades\\ActionButton"
            }
        }
    }
}